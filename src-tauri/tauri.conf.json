{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "enassi",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "fs": {
        "scope": [
          "$RESOURCE/*"
        ]
      }
    },
    "bundle": {
      "active": true,
      "category": "Utility",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.enassi",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [
        "./locales/ar.json",
        "./locales/be-BY.json",
        "./locales/bg-BG.json",
        "./locales/cs-CZ.json",
        "./locales/da-DK.json",
        "./locales/de-DE.json",
        "./locales/el-GR.json",
        "./locales/en.json",
        "./locales/es.json",
        "./locales/fa-IR.json",
        "./locales/fi-FI.json",
        "./locales/fr.json",
        "./locales/he-IL.json",
        "./locales/hi-IN.json",
        "./locales/hu-HU.json",
        "./locales/id-ID.json",
        "./locales/it-IT.json",
        "./locales/ja-JP.json",
        "./locales/kk-KZ.json",
        "./locales/ko-KR.json",
        "./locales/ms.json",
        "./locales/my-MM.json",
        "./locales/nb-NO.json",
        "./locales/nl-NL.json",
        "./locales/pl-PL.json",
        "./locales/pt.json",
        "./locales/ro-RO.json",
        "./locales/ru-RU.json",
        "./locales/sk-SK.json",
        "./locales/sr-SP.json",
        "./locales/sv-SE.json",
        "./locales/th-TH.json",
        "./locales/tr-TR.json",
        "./locales/uk-UA.json",
        "./locales/ur.json",
        "./locales/vi-VN.json",
        "./locales/zh-CHS.json",
        "./locales/zh-CHT.json"
      ],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "label": "main",
        "title": "enassi",
        "height": 600,
        "width": 800,
        "resizable": true,
        "fullscreen": false,
        "decorations": false
      }
    ],
    "systemTray": {
      "iconPath": "icons/32x32.png",
      "iconAsTemplate": true
    }
  }
}